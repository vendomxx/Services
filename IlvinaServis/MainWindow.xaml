<Window x:Class="IlvinaServis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IlvinaServis"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="850" Width="1300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="10*"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Frame x:Name="mainFrame"
               Background="White"
               Grid.Row="1"
               NavigationUIVisibility="Hidden">
        </Frame>
        <Grid Background="#FFE7FABF"></Grid>
        <Grid Background="#FFE7FABF"
              Grid.Row="2"></Grid>
        <DockPanel LastChildFill="False" Grid.Row="0">
            <Button x:Name="closeBtn"
                    Margin="5"
                    Click="closeBtn_Click"
                    DockPanel.Dock="Right">Выход</Button>
            <Button x:Name="goBackBtn"
                    Margin="5"
                    Click="goBackBtn_Click"
                    DockPanel.Dock="Right">Назад</Button>
        </DockPanel>
    </Grid>
</Window>
